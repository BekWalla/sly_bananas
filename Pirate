Pirate = {"sir":"matey", "hotel":"fleabag inn", "student": "swabbie", "boy": "matey", "madam":"proud beauty", "professor": "foul blaggart", "restaurant":"galley", "your":"yer", "excuse": "arr", "students":"swabbies", "are":"be", "lawyer": "foul blaggart", "the":"th'", "restroom":"head", "my": "me", "hello": "avast", "is":"be", "man":"matey"}

def translate(text, wordDict):  
    for key in wordDict:
        text = text.replace(key, wordDict[key])
    return text
